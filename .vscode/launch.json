  "configurations": [
    {
      "name": "(gdb) Linux Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/reproc/test/argv",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
